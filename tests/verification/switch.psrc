/*

<test>
    ~~ 0 = \w* & \(\w* <= 3 or 7 = \w*\) & \(\w* >= 0 or 7 = \w*\) & P_bar\(\w*\) => Q_bar\(\w*\)
    ~~ \(\w* <= 3 or 7 = \w*\) & \(\w* >= 0 or 7 = \w*\) & P_bar\(\w*\) => \(exists nat \w*. 0 = \w*\)
    ~~ 1 = \w* & \(\(!\(\w* <= 3\)\) or \(!\(\w* >= 0\)\)\) & \(\(!\(\w* <= 3\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(\w* >= 0\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(7 = \w*\)\) or \(!\(7 = \w*\)\)\) & \(\w* <= 5 or 8 = \w*\) & \(\w* >= 4 or 8 = \w*\) & P_bar\(\w*\) => Q_bar\(\w*\)
    ~~ \(\(!\(\w* <= 3\)\) or \(!\(\w* >= 0\)\)\) & \(\(!\(\w* <= 3\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(\w* >= 0\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(7 = \w*\)\) or \(!\(7 = \w*\)\)\) & \(\w* <= 5 or 8 = \w*\) & \(\w* >= 4 or 8 = \w*\) & P_bar\(\w*\) => \(exists nat \w*. 1 = \w*\)
    ~~ 2 = \w* & \(\(!\(\w* <= 3\)\) or \(!\(\w* >= 0\)\)\) & \(\(!\(\w* <= 3\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(\w* <= 5\)\) or \(!\(\w* >= 4\)\)\) & \(\(!\(\w* <= 5\)\) or \(!\(8 = \w*\)\)\) & \(\(!\(\w* >= 0\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(\w* >= 4\)\) or \(!\(8 = \w*\)\)\) & \(\(!\(7 = \w*\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(8 = \w*\)\) or \(!\(8 = \w*\)\)\) & P_bar\(\w*\) => Q_bar\(\w*\)
    ~~ \(\(!\(\w* <= 3\)\) or \(!\(\w* >= 0\)\)\) & \(\(!\(\w* <= 3\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(\w* <= 5\)\) or \(!\(\w* >= 4\)\)\) & \(\(!\(\w* <= 5\)\) or \(!\(8 = \w*\)\)\) & \(\(!\(\w* >= 0\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(\w* >= 4\)\) or \(!\(8 = \w*\)\)\) & \(\(!\(7 = \w*\)\) or \(!\(7 = \w*\)\)\) & \(\(!\(8 = \w*\)\) or \(!\(8 = \w*\)\)\) & P_bar\(\w*\) => \(exists nat \w*. 2 = \w*\)
</test>

*/

bar(nat a: nat b)
pre a > 0
{
    switch (a) {
        case 0..3, 7: b = 0
        case 4..5, 8: b = 1
        default: b = 2
    }
}
post b >= 0 & b <= 2;
