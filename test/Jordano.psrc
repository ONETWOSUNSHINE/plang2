module Jordano;

type Matrix (nat rows, cols) = array (real, 1..rows, 1..cols);
type Vector (nat size) = array (real, 1..size);


subtrCol (Matrix (nat n) a, Vector (n) b, nat k : Matrix (n) a', Vector (n) b') {
    b' = b with [ 1..k-1 as i: b[i] - b[k]*a[i, k] ];
    a' = a with [ (1..k-1, k): 0 ];
}
